        <CommandType>Text</CommandType>
        <CommandText>SELECT ROUND(MAX(SALARY),0) AS "Maximo" , ROUND(MIN(SALARY),0) AS "Minimo", ROUND(SUM(SALARY),0) AS "Sumatoria", ROUND(AVG(SALARY),0) AS "Promedio"
FROM HR.EMPLOYEES;</CommandText>
