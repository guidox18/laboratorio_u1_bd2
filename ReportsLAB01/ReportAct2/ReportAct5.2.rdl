      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 
EMPLOYEE_ID as "Nº de empleado"
,LAST_NAME as "Apellidos"
,SALARY as "Salario"
,CONVERT(integer, SALARY + (15.5 * SALARY / 100 ))  as "Nuevo Salario"
FROM HR.EMPLOYEES</CommandText>
